{
  "name": "nodestyle",
  "version": "1.0.0",
  "description": "A Node JS wrapper for the neural style neural network.",
  "main": "main.js",
  "scripts": {
    "test": "mocha",
    "compile": "pyinstaller torchbrain.spec --onedir --noconfirm --workpath=build --distpath=app/bin",
    "precompile": "node ./Scripts/precompile.js",
    "postcompile": "node ./Scripts/copyLicense.js",
    "setup": "node ./Scripts/setup.js",
    "model": "node ./Scripts/acquireModel.js",
    "lint": "eslint . Scripts test --color"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Razerfish/nodestyle.git"
  },
  "keywords": [
    "Pytorch",
    "NeuralStyle"
  ],
  "author": "Alec Wilson",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/Razerfish/nodestyle/issues"
  },
  "homepage": "https://github.com/Razerfish/nodestyle#readme",
  "devDependencies": {
    "colors": "^1.3.3",
    "eslint": "^5.16.0",
    "mocha": "^6.1.4",
    "readline-sync": "^1.4.9",
    "shelljs": "^0.8.3"
  }
}
