    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Node: Console",
            "program": "${file}",
            "console": "internalConsole"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Node: Terminal",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Python: Terminal (integrated)",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Debug Training",
            "type": "python",
            "request": "launch",
            "program": "${workspaceRoot}/torchbrain/torchbrain.py",
            "console": "integratedTerminal",
            "args": ["neural_style", "{\"subcommand\":\"train\", \"dataset\":\"./test/Testing_Assets/dataset\", \"style_image\":\"./test/Testing_Assets/style_image.jpg\", \"save_model_dir\":\"./test/output\", \"name\":\"Test_Model\", \"cuda\":1}"]
        },
        {
            "name": "Debug Stylization",
            "type": "python",
            "request": "launch",
            "program": "${workspaceRoot}/torchbrain/torchbrain.py",
            "console": "integratedTerminal",
            "args": ["neural_style", "{\"subcommand\":\"eval\", \"content_image\":\"./test/Testing_Assets/content.jpg\", \"output_image\":\"./test/output/Test_Output.jpg\", \"model\":\"./test/Testing_Assets/model.pth\", \"cuda\":1}"]
        },
        {
            "name": "Debug check_cuda",
            "type": "python",
            "request": "launch",
            "program": "${workspaceRoot}/torchbrain/torchbrain.py",
            "console": "integratedTerminal",
            "args": ["check_cuda"]
        }
    ]
}